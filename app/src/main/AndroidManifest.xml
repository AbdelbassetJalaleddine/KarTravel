<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jalaleddine.abdelbasset.kartravel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_plane"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_plane"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AddEmployeeActivity"></activity>
        <activity android:name=".EmployeesActivity" />
        <activity android:name=".VerifyActivity" />
        <activity android:name=".TrackerActivity" />
        <activity android:name=".PackagesActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".VerifyPhoneActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>